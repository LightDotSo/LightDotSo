{
  "$schema": "https://raw.githubusercontent.com/awjh/license-check-and-add/master/config-schema.json",
  "ignore": [
    "**/CHANGELOG.md",
    "**/README.md",
    "**/.DS_STORE",
    "**/generated.ts",
    "**/plonk_vk.sol",
    "**/Package.swift",
    "**/prisma/src/lib.rs",
    "**/schema-rs.prisma",
    "**/schema.prisma",
    "**/.gitkeep",
    "**/.gitignore",
    "**/.*rc.js",
    "**/.*ignore",
    "**/.gradle*",
    "**/.pnpm*",
    "**/*.css",
    "**/*.config.js",
    "**/*.config.cjs",
    "**/*.config.ts",
    "**/*.entitlements",
    "**/*.gradle",
    "**/*.html",
    "**/*.icns",
    "**/*.lock",
    "**/*.pbxproj",
    "**/*.png",
    "**/*.pro",
    "**/*.proof",
    "**/*.toml",
    "**/*.plist",
    "**/*.resolved",
    "**/*.svg",
    "**/*.xcscheme",
    "**/*.xcsettings",
    "**/*.xcuserstate",
    "**/*.xcworkspacedata",
    "**/*.xml",
    "**/*.webp",
    "**/next-env.d.ts",
    "**/vite-env.d.ts",
    "gradle*",
    "pnpm*",
    "*.gradle",
    "*file",
    ".*",
    "*.config.ts",
    "*.workspace.ts",
    "*.json",
    "*.kdl",
    "*.md",
    "*.toml",
    "*.txt",
    "*.yml",
    "apps/extension/**/*.js",
    "ios/LightWalletSafariExtension/Resources/**/*.js",
    "**/*.app/**",
    "**/.cargo/**",
    "**/.changeset/**",
    "**/.husky/**",
    "**/.github/**",
    "**/.mobiledev/**",
    "**/.storybook/**",
    "**/_next/**",
    "**/*.appex/**",
    "**/*.xcframework/**",
    "**/.build/**",
    "**/.gradle/**",
    "**/.idea/**",
    "**/.next/**",
    "**/.swiftpm/**",
    "**/.turbo/**",
    "**/build/**",
    "**/coverage/**",
    "**/debug/**",
    "**/fastlane/**",
    "**/gradle/**",
    "**/lib/**",
    "**/pods/**",
    "**/scripts/**",
    "**/target/**",
    "**/tools/**"
  ],
  "license": "LICENSE.header",
  "defaultFormat": {
    "eachLine": {
      "prepend": "# "
    },
    "append": ""
  },
  "licenseFormats": {
    "js|ts|tsx|rs|sol|swift|kt|nr|udl": {
      "eachLine": {
        "prepend": "// "
      },
      "append": ""
    },
    "mdx": {
      "prepend": "{/*",
      "append": "*/}\n"
    },
    "sh": {
      "eachLine": {
        "prepend": "# "
      },
      "append": ""
    }
  },
  "trailingWhitespace": "TRIM"
}
