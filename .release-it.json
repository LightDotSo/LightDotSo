{
  "git": {
    "commitMessage": "chore: release v${version} [skip ci]",
    "requireUpstream": false,
    "tagName": "v${version}"
  },
  "github": {
    "autoGenerate": true,
    "release": true,
    "releaseName": "v${version}",
    "web": true
  },
  "plugins": {
    "@release-it/keep-a-changelog": {
      "filename": "CHANGELOG.md",
      "strictLatest": false
    }
  }
}
