---
source: crates/graphql/src/polling/light_wallets.rs
expression: query.query
---
query GetLightWalletsQuery($minBlock: BigInt!, $minIndex: BigInt!) {
  lightWallets(where: {blockNumber_gte: $minBlock, index_gt: $minIndex, }, orderBy: index) {
    id
    index
    address
    imageHash
    userOpHash
    sender
    factory
    paymaster
    blockNumber
    blockTimestamp
    transactionHash
  }
  _meta {
    block {
      number
    }
  }
}


