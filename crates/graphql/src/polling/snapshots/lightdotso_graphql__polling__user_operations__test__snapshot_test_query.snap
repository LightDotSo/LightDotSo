---
source: crates/graphql/src/polling/user_operations.rs
expression: query.query
---
query GetUserOperationsQuery($minIndex: BigInt!) {
  userOperations(first: 300, where: {index_gt: $minIndex, }, orderBy: index) {
    id
    index
    sender
    nonce
    initCode
    callData
    callGasLimit
    verificationGasLimit
    preVerificationGas
    maxFeePerGas
    maxPriorityFeePerGas
    paymasterAndData
    signature
    entryPoint
    userOperationEvent {
      id
      index
      userOpHash
      paymaster
      nonce
      transactionHash
    }
    userOperationRevertReason {
      id
      index
      userOpHash
      sender
      nonce
      revertReason
      transactionHash
    }
    lightWallet {
      id
      index
      address
      imageHash
      userOpHash
      sender
      factory
      paymaster
      blockNumber
      blockTimestamp
      transactionHash
    }
  }
  _meta {
    block {
      number
    }
  }
}


