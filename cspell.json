{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "dictionaries": [
    "project-words"
  ],
  "dictionaryDefinitions": [
    {
      "name": "project-words",
      "path": "./project-words.txt",
      "addWords": true
    }
  ],
  "useGitignore": true,
  "ignorePaths": [
    "node_modules",
    "pnpm-lock.yaml",
    "assets/meta",
    "**.sql"
  ],
  "enableFiletypes": [
    "mdx"
  ],
  "enabled": true
}