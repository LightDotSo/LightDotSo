{
  "name": "light-wallet",
  "private": true,
  "version": "0.0.1-0",
  "description": "Official Repository for Light Wallet",
  "scripts": {
    "build": "turbo run build",
    "build:dev": "turbo run build:dev",
    "changeset": "changeset",
    "eslint:cmd": "eslint --config .eslintrc.js --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint",
    "eslint:fix": "eslint --config .eslintrc.js --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint . --fix",
    "eslint:lint": "eslint --config .eslintrc.js --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "fastlane:build": "cd ios && bundle exec fastlane build",
    "fastlane:test": "cd ios && bundle exec fastlane test",
    "fix": "concurrently 'pnpm:*:fix'",
    "lint": "echo \"Error: no lint specified\" && exit 0",
    "lint:eslint": "pnpm run eslint:lint",
    "lint:license": "make license-checker",
    "lint:prettier": "pnpm run prettier:lint",
    "lint:swiftlint": "pnpm run swiftlint:lint",
    "lint:swift-format": "pnpm run swift-format:lint",
    "ncu:check": "ncu--packageFile 'packages/**/package.json'",
    "ncu:upgrade:monorepo": "ncu --packageFile 'packages/**/package.json' -u",
    "ncu:upgrade:root": "ncu --packageFile package.json -u",
    "ncu:upgrade": "concurrently 'pnpm:ncu:upgrade:*'",
    "prettier:cmd": "prettier",
    "prettier:fix": "prettier --write .",
    "prettier:lint": "prettier --check .",
    "release": "changeset publish",
    "release:root": "cross-env HUSKY=0 pnpm run release-it",
    "swiftlint:cmd": "mint run swiftlint",
    "swiftlint:fix": "mint run swiftlint --fix",
    "swiftlint:lint": "mint run swiftlint",
    "swift-format:cmd": "mint run swift-format --in-place --recursive --parallel --ignore-unparsable-files",
    "swift-format:fix": "mint run swift-format --in-place --recursive --parallel --ignore-unparsable-files ios/**/Sources/**",
    "swift-format:lint": "mint run swift-format --recursive --parallel --ignore-unparsable-files ios/**/Sources/**",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@release-it/keep-a-changelog": "^3.1.0",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "autoprefixer": "^10.4.14",
    "commitlint": "^17.5.1",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.14",
    "eslint": "^8.37.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-turbo": "^1.8.8",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-editorconfig": "^4.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "3.10.1",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-check-updates": "^16.9.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "release-it": "^15.9.3",
    "tailwindcss": "^3.3.0",
    "turbo": "^1.8.8",
    "typescript": "^5.0.2",
    "vercel": "^28.18.3"
  },
  "packageManager": "pnpm@7.24.3"
}
