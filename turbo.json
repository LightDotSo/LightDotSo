{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["DISABLE_EXTRACTION", "NODE_ENV", "EAS_BUILD_PLATFORM"],
  "pipeline": {
    "build": {
      "env": ["DISABLE_EXTRACTION", "NODE_ENV", "EAS_BUILD_PLATFORM"],
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "build/**", "node_modules/.cache/metro/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "//#ci": {
      "dependsOn": [],
      "outputs": [],
      "inputs": ["**"]
    }
  }
}
